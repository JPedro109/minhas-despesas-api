{
  "name": "minhas-despesas-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Jo√£o Pedro <joaopedromoraesnsilva@gmail.com>",
  "repository": "https://github.com/JPedro109/minhas-despesas-api.git",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky init",
    "test": "jest --passWithNoTests --detectOpenHandles --silent",
    "test:coverage": "yarn test --coverage",
    "test:staged": "yarn test --findRelatedTests",
    "test:watch": "yarn test --watchAll",
    "test:unit": "yarn test -c jest-unit-config.js",
    "test:integration": "yarn test -c jest-integration-config.js",
    "test:unit:watch": "yarn test --watchAll -c jest-unit-config.js",
    "test:integration:watch": "yarn test --watchAll -c jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles",
    "test:watch:verbose": "yarn test:verbose --watchAll",
    "test:unit:verbose": "yarn test:verbose -c jest-unit-config.js",
    "test:integration:verbose": "yarn test:verbose -c jest-integration-config.js",
    "test:unit:watch:verbose": "yarn test:verbose --watchAll -c jest-unit-config.js",
    "test:integration:watch:verbose": "yarn test:verbose --watchAll -c jest-integration-config.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/js": "^9.9.1",
    "@types/jest": "^29.5.12",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  }
}
